<body id="page-top">
    <div id="wrapper">
        <%include sidebar%>
        <div class="container">
            <%include header%>
                <div class="row justify-content-center">
                    <div class="col-xl-10 col-lg-12 col-md-9">
                        <div class="card o-hidden border-0 shadow-lg my-5">
                            <div class="card-body p-0">  
                                <form action="" method="post">
                                    <label for="">select Room type:</label>
                                    <input type="radio" name="roomType" value="1" id="">single
                                    <input type="radio" name="roomType" value="2">double
                                    <input type="radio" name="roomType" value="3">triple
                                    <input type="radio" name="roomType" value="4">four People Room
                                    <br>
                                    <label for="">price</label>
                                    <input type="number" name="price" id="">
                                    <br>
                                    <button type="submit">update</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>              
        </div>
    </div>
    
</body>
</html>