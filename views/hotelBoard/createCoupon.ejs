<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Page Title</title>
    <link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="/css/sb-admin-2.min.css" rel="stylesheet">
</head>
<body id="page-top">
    <div id="wrapper">
        <% include sidebar%>
        <div class="container">
            <%include header%>
                <div class="row justify-content-center">    
                        <div class="col-xl-10 col-lg-12 col-md-9">
                          <div class="card o-hidden border-0 shadow-lg my-5">
                            <div class="card-body p-0">
                                <h1>create coupon</h1>
                                <form action="" method="post">
                                    <div>
                                        <label for="">code</label>
                                        <input type="text" name="code">
                                    </div>
                                    <div>
                                        <label for="">Description</label>
                                        <input type="text" name="description">
                                    </div>
                                    <div>
                                            <label for="">Discount</label>
                                            <input type="number" name="discount">
                                        </div>

                                    <div>
                                        <label for="">Discount Type</label>
                                        <select name="discountType" id="">
                                            <option value="rs">%</option>
                                            <option value="$">rs</option>
                                        </select>
                                    </div>
                                    <div>
                                        <label for="">validfrom</label>
                                        <input type="date" name="validFrom">
                                      
                                    </div>
                                    <div>
                                        <label for="">validUntil</label>
                                        <input type="date" name="validUntil">
                                    </div>
                                    <div>
                                        <label for="">min value</label>
                                        <input type="number" name="minValue">
                                    </div>
                                    <div>
                                            <label for="">max value</label>
                                            <input type="number" name="maxValue">
                                    </div>
                                    <button type="submit">create Coupon</button>
                                </form>

                            </div>
                          </div>
                        </div>
                  </div>              
        </div>
    </div>
</body>
</html>