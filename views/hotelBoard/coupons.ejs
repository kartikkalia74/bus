<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Page Title</title>
        <link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
        <!-- Custom styles for this template-->
        <link href="/css/sb-admin-2.min.css" rel="stylesheet">
        <style>
            table, th, td {
            border: 1px solid black;
        }
        </style>
    </head>
    <body id="page-top">
        <div id="wrapper">
            <% include sidebar%>
            <div class="container">
                <%include header%>
                    <a href="createCoupon">createCoupon</a>
                    <div class="row justify-content-center">    
                            <div class="col-xl-10 col-lg-12 col-md-9">
                            <div class="card o-hidden border-0 shadow-lg my-5">
                                <div class="card-body p-0">
                                    <h1>coupons</h1>
                                    <table>
                                        <tr style="border:2px">
                                            <th>code</th>
                                            <th>description</th>
                                            <th>discount</th>
                                            <th>discountType</th>
                                            <th>validFrom</th>
                                            <th>validUntil</th>
                                            <th>minValue</th>
                                            <th>maxValue</th>
                                        </tr>
                                        <%couponsList.forEach(function(coupon){%>
                                            <tr> <td><%=coupon.code%></td> 
                                                <td><%=coupon.description%></td>
                                                <td><%=coupon.discount%></td>
                                                <td><%=coupon.discountType%></td>
                                                <td><%=coupon.validFrom%></td>
                                                <td><%=coupon.validUntil%></td>
                                                <td><%=coupon.minValue%></td>
                                                <td><%=coupon.maxValue%></td>
                                                <td><a href= <%="coupon/" + coupon.code %>>delete</a></td>
                                            </tr> 
                                       <% }) %>
                                        
                                    </table>
                                </div>
                            </div>
                            </div>
                    </div>              
            </div>
        </div>
    </body>
</html>